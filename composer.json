{
    "name": "makinacorpus/corebus",
    "description": "Command and event buses interface and logic.",
    "homepage": "http://github.com/makinacorpus/php-corebus",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Makina Corpus",
            "homepage": "https://makina-corpus.com/"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "makinacorpus/event-store": "^1.0.4",
        "makinacorpus/goat-query-bundle": "^3.1",
        "makinacorpus/message": "^1.0.3",
        "makinacorpus/message-broker": "^1.0.3",
        "phpunit/phpunit": "^9.2",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\CoreBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
             "MakinaCorpus\\CoreBus\\Tests\\": "tests/"
        }
    }
}
